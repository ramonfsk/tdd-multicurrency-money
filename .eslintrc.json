{
    "env": {
        "es2021": true,
        "node": true,
        "jest": true
    },
    "extends": "standard",
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "no-unused-vars": [
            "error",
          { "varsIgnorePattern": "^[A-Z]" }
        ]
    }
}
